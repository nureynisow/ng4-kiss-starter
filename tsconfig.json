{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": ["es2015", "dom"],
		"noImplicitAny": false,
		"skipLibCheck": true,
		"types": ["core-js", "jasmine", "node", "lodash"]
	}
}
